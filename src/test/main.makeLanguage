var e = 0
while(e <= 15){
    ++e
    if(e == 10){
        break
    }
    if(e == 5){
        continue
    }
}
print(e)
for(var i = 0; i <= 15; ++i){
    --e
    if(e == 0) {
        break
    }
    if(i == 8){
        continue
    }
}
print(e)
